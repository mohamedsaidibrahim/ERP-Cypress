{
  "name": "ERP_MicroTec_Cypress",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:run:chrome": "npx cypress run --browser chrome",
    "cy:chrome:headed": "npx cypress run --browser chrome --headed",
    "cy:run:firefox": "npx cypress run --browser firefox",
    "cy:firefox:headed": "npx cypress run --browser firefox --headed",
    "cy:run:edge": "npx cypress run --browser edge",
    "cy:edge:headed": "npx cypress run --browser edge --headed",
    "cy:merge-reports": "npx mochawesome-merge cypress/reports/*.json > mergedReport.json",
    "cy:generate-report": "npx marge mergedReport.json -o cypress/reports",
    "all:reports": "npx cypress-reporter",
    "cy:run_report": "npm run cy:run:chrome && npx cypress-reporter",
    "cy:run_report_headed": "npm run cy:chrome:headed && npm run cy:merge-reports && npm run cy:generate-report",
    "cy:auth": "npx cypress run --browser chrome  --spec 'cypress/e2e/Authentication/**/*.cy.ts'",
    "cy:add_domain_space": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/My-subscriptions/Add_domain_space/**/*.cy.ts'",
    "cy:companies": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/My-subscriptions/Manage_companies/**/*.cy.ts'",
    "cy:branches": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/My-subscriptions/manage_branches/**/*.cy.ts'",
    "cy:manage_users": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/My-subscriptions/Manage_users/**/*.cy.ts'",
    "cy:manage_apps": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/My-subscriptions/Manage_apps/**/*.cy.ts'",
    "cy:general_setting": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/Erp_Login/General_Setting/**/*.cy.ts'",
    "cy:accounting": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/Erp_Login/Accounting/**/*.cy.ts'",
    "cy:hr": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/Erp_Login/HR/**/*.cy.ts'",
    "cy:finance": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/Erp_Login/Finance/**/*.cy.ts'",
    "cy:sales": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/Erp_Login/Sales/**/*.cy.ts'",
    "cy:purchase": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/Erp_Login/Purchase/**/*.cy.ts'",
    "cy:chart_of_account": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/Erp_Login/Accounting/charts_of_accounts/*.cy.ts'",
    "cy:tax_gp": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/Erp_Login/Accounting/Taxs Group/*.cy.ts'",
    "cy:tax_def": "npx cypress run --browser chrome  --spec 'cypress/e2e/Bussiness_owners/Erp_Login/Accounting/Taxs Definition/*.cy.ts'",
    "show_report": "npx cmur && node node_modules/mochawesome_responsive_report/src/index.js",
    "js_console": "npx cypress run --browser chrome --spec 'cypress/e2e/Global_tests/navigationAndErrorDetection/navigationAndErrorDetection.cy.ts'",
    "js_errors": "npx cypress run --browser chrome --spec 'cypress/e2e/Global_tests/Javascript_errors/tests/js_errors.cy.ts'",
    "broken_links": "npx cypress run --browser chrome --spec 'cypress/e2e/Broken_Links/tests/erp_broken_links_spec.cy.ts'",
    "broken_links_csv": "npx cypress run --browser chrome --spec 'cypress/e2e/Broken_Links/tests/erp_broken_links_csv.cy.ts'",
    "broken_links_json": "npx cypress run --browser chrome --spec 'cypress/e2e/Broken_Links/tests/erp_broken_links_json.cy.ts'",
    "page:load_time": "npx cypress run --browser chrome --spec 'cypress/e2e/Global_tests/PERFORMANCE/page_load_time/page_load_time.cy.ts'",
    "axa": "npx cypress run --browser chrome --spec 'cypress/e2e/Global_tests/Accessbility/Archive/all_urls/version1_generate-accessibility-report.cy.js'",
    "cy:access": "npx cypress run --browser chrome --spec 'cypress/e2e/Global_tests/Accessbility/tests/accessibility-test.cy.js'",
    "v1:access": "npx cypress run --browser chrome --spec 'cypress/e2e/Global_tests/Accessbility/Archive/Version1/accessibility-test.cy.js'",
    "build:image": "docker build -t erp_microtec_cypress .",
    "run:container": "docker run -it --rm erp_microtec_cypress",
    "docker:compose": "docker-compose up --build"
  },
  "version": "1.0.0",
  "main": "index.js",
  "license": "Apache-2.0",
  "author": {
    "name": "Mohamed Said Ibrahim",
    "email": "muhammed.saeed@microtec.com.eg"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.1",
    "@types/cypress": "^1.1.3",
    "@types/node": "^20.16.12",
    "cheerio": "^1.0.0-rc.12",
    "cypress": "^13.10.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^2.0.1",
    "fast-csv": "^5.0.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "path": "^0.12.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "csv-writer": "^1.6.0",
    "cypress-reportify": "^1.2.0",
    "ERP_MicroTec_Cypress": "file:"
  }
}
